<br />

<div class="booyah-box col-xs-10 col-xs-offset-1">
  <div class="chessboard">
    <% (0..7).each do |row| %>
      <% (0..7).each do |col| %>
        <div class="square <%= (row + col) % 2 == 0 ? "light" : "dark" %>"
          id="<% row.to_s + col.to_s %>"></div>
      <% end %>
    <% end %>
  </div>
</div>
